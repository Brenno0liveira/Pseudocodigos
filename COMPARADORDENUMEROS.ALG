Algoritmo "Comparadordenumeros"

Var
   num1,num2: Real



Inicio
   escreva("Digite um número: ")
   leia(num1)
   escreva("Digite outro número: ")
   leia(num2)
   
   se (num1>num2) entao
      escreva("O primeiro número é maior")
   senao
      escreva("o segundo número é maior")
   fimse
Fimalgoritmo